{
  "author": "Aidan647",
  "name": "@stationeers-ic/exact-ic10-math",
  "description": "A library that faithfully implements IC10 math, bitwise, and logic instructions with exact behavior.",
  "version": "1.1.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "format": "prettier . --write",
    "watch": "bun test --watch",
    "test": "bun test",
    "build": "bun run build:esm && bun run build:cjs",
    "build:esm": "tsc --removeComments && tsc --declaration --emitDeclarationOnly",
    "build:cjs": "tsc --project tsconfig.cjs.json --removeComments && bun scripts/move-cjs.ts",
    "prepack": "bun test && rm -R dist && bun run build",
    "updateHashes": "bun updateHashesTest.ts"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stationeers-ic/exact-ic10-math.git"
  },
  "keywords": [
    "typescript",
    "ic10",
    "math",
    "assembly",
    "bitwise",
    "logic",
    "stationeers",
    "emulator",
    "exact"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stationeers-ic/exact-ic10-math/issues"
  },
  "homepage": "https://github.com/Stationeers-ic/exact-ic10-math#readme",
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
